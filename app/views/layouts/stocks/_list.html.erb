<div class="row">
    <div class="col-9">
        <table class="table">
        <thead>
            <tr>
            <th scope="col">Ticker</th>
            <th scope="col">Name</th>
            <th scope="col">Price</th>
            <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
            <%= @tracked_stocks.each do |stock| %>
                <tr>
                    <th scope="row"><%= stock.ticker %></th>
                    <td><%= stock.name %></td>
                    <td><%= stock.last_price%></td>
                    <td>Action Bitch</td>
                </tr>
            <% end %>
        </tbody>
        </table>
    </div>
</div>